{
  "name": "tori-telegram-bot",
  "version": "1.0.0",
  "description": "Telegram bot made to follow tori queries.",
  "main": "dist/app.js",
  "repository": "https://github.com/EetuK/tori-telegram-bot.git",
  "author": "EetuK",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "node dist/app.js",
    "develop": "concurrently -k -i -p \"[{name}]\" -n \"Node,TypeScript\" -c \"yellow.bold,cyan.bold\" \"npm run watch-js\" \"npm run watch-ts\"",
    "build-ts": "tsc -p .",
    "watch-ts": "tsc -w --skipLibCheck",
    "watch-js": "nodemon dist/app.js"
  },
  "devDependencies": {
    "@types/node": "^14.14.35",
    "concurrently": "^6.0.0",
    "nodemon": "^2.0.7"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "lowdb": "^2.1.0",
    "telegraf": "^4.3.0",
    "typescript": "^4.2.3",
    "winston": "^3.3.3",
    "x-ray": "^2.3.4"
  }
}
